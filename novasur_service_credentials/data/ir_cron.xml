<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="ir_cron_check_expired_credentials" model="ir.cron">
    <field name="name">Credenciales: Marcar Expiradas</field>
    <field name="model_name">service.credentials</field>
    <field name="state">code</field>
    <field name="code">model.cron_check_expired_credentials()</field>
    <field name="user_id" ref="base.user_admin"/>
    <field name="interval_number">1</field>
    <field name="interval_type">days</field>
    <field name="numbercall">-1</field>
    <field name="doall">False</field>
    <field name="active">True</field>
    <field name="priority">5</field>
    </record>

    <record id="ir_cron_warn_expiring_soon" model="ir.cron">
    <field name="name">Credenciales: Advertir Pr√≥ximas a Expirar</field>
    <field name="model_name">service.credentials</field>
    <field name="state">code</field>
    <field name="code">model.cron_warn_expiring_soon(days_before=7)</field>
    <field name="user_id" ref="base.user_admin"/>
    <field name="interval_number">1</field>
    <field name="interval_type">days</field>
    <field name="numbercall">-1</field>
    <field name="doall">False</field>
    <field name="active">False</field>
    <field name="priority">10</field>
    </record>

  </data>
</odoo>
